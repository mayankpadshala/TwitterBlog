{
  "name": "microblab",
  "version": "1.0.0",
  "description": "Social network for developers",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider node server",
    "server": "cross-env NODE_OPTIONS=--openssl-legacy-provider nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "env": "echo $NODE_OPTIONS",
    "build": "npm install && npm install --prefix client && npm run build --prefix client"
  },
  "author": "MicroBlabers",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.5",
    "client": "file:client",
    "config": "^3.3.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.8.1",
    "gravatar": "^1.8.1",
    "http-proxy-middleware": "^2.0.6",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "normalize-url": "^5.3.0",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "qrcode": "^1.5.3",
    "redis": "^4.6.11",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
